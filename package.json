{
  "version": "2.0.0",
  "name": "@moj/moj-ng",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4400 --proxy-config proxy.config.json",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "doc:build": "compodoc -p tsconfig.documentation.json -a screenshots -y doc-styles/ --disableGraph --disableCoverage --disableSourceCode --disablePrivate --disableProtected --disableInternal --disableLifeCycleHooks --includes markdown",
    "doc:serve": "compodoc -s",
    "doc:buildandserve": "compodoc -p tsconfig.documentation.json -a screenshots -y doc-styles/ -s --disableGraph --disableCoverage --disableSourceCode --disablePrivate --disableProtected --disableInternal --disableLifeCycleHooks --includes markdown",
    "generate-barrels": "barrelsby --delete --directory src/app/moj-ng --name index --exclude .*\\.spec\\.ts",
    "start:demo": "npm run generate-barrels && ng s --project=demo --port 2000",
    "build:demo:prod": "npm run generate-barrels && ng build --prod --project=demo"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "8.0.0",
    "@angular/compiler-cli": "8.0.0",
    "@compodoc/compodoc": "1.1.7",
    "@types/node": "8.9.4",
    "barrelsby": "^1.0.2",
    "ng-packagr": "^4.1.1",
    "node-sass": "^4.9.2",
    "typescript": "3.4.3"
  },
  "engines": {
    "node": ">=9.4.0"
  },
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/cdk": "8.0.0",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/forms": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/router": "8.0.0",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "2.0.0",
    "ag-grid-angular": "19.1.1",
    "ag-grid-community": "19.1.1",
    "ag-grid-enterprise": "19.1.1",
    "angular2-text-mask": "^9.0.0",
    "bootstrap": "3.4.1",
    "bootstrap-rtl": "3.3.4",
    "chart.js": "^2.9.3",
    "core-js": "2.5.1",
    "es6-shim": "0.35.3",
    "idsp-base-web-seed": "0.0.52",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "libphonenumber-js": "0.4.27",
    "ng-click-outside": "^5.1.1",
    "ng2-validation": "4.2.0",
    "primeicons": "1.0.0-beta.10",
    "primeng": "^8.1.1",
    "quill": "1.3.6",
    "reflect-metadata": "0.1.10",
    "rxjs": "6.2.2",
    "stacktrace-js": "2.0.0",
    "systemjs": "0.20.19",
    "systemjs-plugin-json": "0.3.0",
    "tslib": "1.9.0",
    "webpack": "^4.41.5",
    "zone.js": "0.8.26"
  }
}
